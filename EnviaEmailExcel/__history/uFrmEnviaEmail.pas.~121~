        qryConsulta.FieldByName('Vda Loja_15').AsString;
      fdVdaMes.FieldByName('Vda_jaq15').value :=
        qryConsulta.FieldByName('Vda jaq_15').AsString;
      fdVdaMes.FieldByName('%jaq15').value :=
        qryConsulta.FieldByName('%Jaq_15').AsString;
      fdVdaMes.Post;
