      FormatFloat('###,###,###,###', cTotalVdaLoja3) + '</b></td>';
    sHtml := sHtml + '<td style="background-color: powderblue;"><b>' +
      FormatFloat('###,###,###,###', cTotalVdaJaq3) + '</b></td>';
    if cTotalJaq3 <> 0 then
