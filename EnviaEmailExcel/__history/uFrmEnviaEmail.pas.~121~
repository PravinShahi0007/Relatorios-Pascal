sHtml := sHtml + '<td style="text-align:left;width:20.73mm; " class="Default"> </td>                         ';
sHtml := sHtml + '<td style="text-align:left;width:20.73mm; " class="Default"> </td>                         ';
sHtml := sHtml + '<td style="text-align:left;width:14.83mm; " class="ce26"> </td>                            ';
sHtml := sHtml + '<td style="text-align:left;width:20.73mm; " class="Default"> </td>                         ';
sHtml := sHtml + '<td style="text-align:left;width:20.73mm; " class="Default"> </td>                         ';
sHtml := sHtml + '<td style="text-align:left;width:14.83mm; " class="ce26"> </td>                            ';
sHtml := sHtml + '<td style="text-align:left;width:20.73mm; " class="Default"> </td>                         ';
sHtml := sHtml + '<td style="text-align:left;width:20.73mm; " class="Default"> </td>                         ';
sHtml := sHtml + '<td style="text-align:left;width:14.83mm; " class="ce26"> </td>                            ';
sHtml := sHtml + '<td style="text-align:left;width:20.73mm; " class="Default"> </td>                         ';
sHtml := sHtml + '<td style="text-align:left;width:20.73mm; " class="Default"> </td>                         ';
sHtml := sHtml + '<td style="text-align:left;width:14.83mm; " class="ce26"> </td>                            ';
sHtml := sHtml + '<td style="text-align:left;width:17mm; " class="Default"> </td>                            ';
sHtml := sHtml + '</tr>                   ';
sHtml := sHtml + '</table>                ';
sHtml := sHtml + '</body>                 ';
sHtml := sHtml + '</html>                 ';


     AssignFile(f,'c:\arquivo_qualquer.txt');
     Rewrite(f); //abre o arquivo para escrita
     Writeln(f,'Testando'); //escreve no arquivo e desce uma linha
     Write(f,sHtml); //escreve no arquivo sem descer a linha
     Closefile(f); //fecha o handle de arquivo


























    qry.Active := False;
    qry.SQL.Clear;
    qry.SQL.Text := 'SELECT * FROM GRZ_FORM_EMAIL g               ' +
                    'WHERE TRUNC(DATA_CADASTRO) = TRUNC(sysdate) ' +
                    'AND g.form_origem = ''Auto''                ' +
                    'AND g.ind_status = 0                        ';
    qry.Active := true;
    qry.RecordCount;
    sSeqChave := qry.FieldByName('seq_chave').AsString;

    qryUpdate.Active := False;
    qryUpdate.SQL.Clear;
    try
      qryUpdate.SQL.Text := ' UPDATE GRZ_FORM_EMAIL     ' +
                            ' SET ind_status = 1,       ' +
                            '     html = :sHtml         ' +
                            ' WHERE ind_status = 0      ' +
                            ' AND SEQ_CHAVE = ' + sSeqChave;
      qryUpdate.ParamByName('sHtml').Size := 999999;
     // qryUpdate.ParamByName('sHtml').AsBlob := sHtml
      qryUpdate.ParamByName('sHtml').LoadFromFile('C:\arquivo_qualquer.txt', ftBlob);

    except
      on e: Exception do
      begin
        ShowMessage(e.Message);
        exit;
      end;
    end;
    qryUpdate.ExecSQL;
end;


procedure TForm1.Log(texto: string);
Var
  sArq: string;
  f: TextFile;
  fb: file of byte;
  iSize: Integer;
begin
  sArq := sDiretorio + '\log.log';
  try
    try
      if not FileExists(sArq) then
      Begin
        Rewrite(f, sArq);
        CloseFile(f);
      end;
      AssignFile(fb, sArq);
      Reset(fb);
      iSize := FileSize(fb);
      CloseFile(fb);
      if iSize > 1048576 then
        DeleteFile(pChar(sArq));
      AssignFile(f, sArq);
      if not FileExists(sArq) then
        Rewrite(f, sArq);
      Append(f);
      Writeln(f, '---->' + FormatDateTime('dd/mm/yyyy hh:nn:ss:zzz', now) +
        ':' + texto);
    finally
      CloseFile(f);
    end;
  except
    exit;
  end;

end;

function TForm1.PrencheTabelaMemoriaLoja: Boolean;
begin
  qry2.First;
  while not qry2.Eof do
  begin
    fdVdaMesLoja.Active := true;
    fdVdaMesLoja.Append;
    fdVdaMesLoja.FieldByName('Regiao').value :=
      qry2.FieldByName('Região').AsString;
    fdVdaMesLoja.FieldByName('nome').value := qry2.FieldByName('Nome').AsString;
    fdVdaMesLoja.FieldByName('Unidade').value :=
      qry2.FieldByName('Unidade').AsString;
    fdVdaMesLoja.FieldByName('rede').value := qry2.FieldByName('Rede').AsString;
    fdVdaMesLoja.FieldByName('vda_loja').value :=
      qry2.FieldByName('Vda Loja').AsString;
    fdVdaMesLoja.FieldByName('Vda_jaq').value :=
      qry2.FieldByName('Vda jaq').AsString;
    fdVdaMesLoja.FieldByName('%jaq').value := qry2.FieldByName('%Jaq').AsString;

    fdVdaMesLoja.FieldByName('vda_loja1').value :=
      qryConsulta.FieldByName('Vda Loja_1').AsString;
    fdVdaMesLoja.FieldByName('vda_loja2').value :=
      qryConsulta.FieldByName('Vda Loja_2').AsString;
    fdVdaMesLoja.FieldByName('vda_loja3').value :=
      qryConsulta.FieldByName('Vda Loja_3').AsString;
    fdVdaMesLoja.FieldByName('vda_loja4').value :=
      qryConsulta.FieldByName('Vda Loja_4').AsString;
    fdVdaMesLoja.FieldByName('vda_loja5').value :=
      qryConsulta.FieldByName('Vda Loja_5').AsString;
    fdVdaMesLoja.FieldByName('vda_loja6').value :=
      qryConsulta.FieldByName('Vda Loja_6').AsString;
